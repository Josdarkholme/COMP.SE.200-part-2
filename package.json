{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls" 
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "blanket": "^1.2.3",
    "chai": "^4.3.7",
    "lint": "^1.1.2",
    "mocha": "^10.1.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "src/.internal/",
        "coverage"
      ]
    }
  },
  "nyc": {
    "all": true,
    "per-file": true,
    "include" : [
      "src/*.js*"
    ]
  }
}
